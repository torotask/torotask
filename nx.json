{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "projects": {
    "client": "packages/client",
    "server": "packages/server",
    "tester": "apps/tester",
    "dashboard": "apps/dashboard"
  },
  "targetDefaults": {
    "dev": {
      "dependsOn": ["^dev"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "lint": {
      "cache": true
    }
  }
}
